        <arguments>
          <argument>
            <kind>1</kind>
            <string>//Get the change of the mouse position
change_x = (display_mouse_get_x()-display_get_width()/2);
change_y = (display_mouse_get_y()-display_get_height()/2);

direction -= change_x/10;
zdirection -= change_y/10;

//Reset the mouse
display_mouse_set(display_get_width()/2,display_get_height()/2);

// set the projection 
xt=x+cos(degtorad(direction));
yt=y-sin(degtorad(direction));
zt=z+tan(degtorad(zdirection));
d3d_set_projection(x,y,z,    xt,yt,zt,   0,0,1);

//Draw da flawr
d3d_draw_floor(0,0,0,800,800,0,global.TEXTURE['grass'],8,8);

//Draw the sky
d3d_draw_ellipsoid(x-400,y-400,z-400,x+400,y+400,z+400,global.TEXTURE['sky'],1,1,12);
</string>
