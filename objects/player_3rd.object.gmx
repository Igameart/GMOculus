x2 = x+5
y1 = y-5
y2 = y+5

ang = -1;
for(var i=0;i&lt;=360;i+=45){

    if (line_col(obj_room.list,x,y,z+hiz,x+lengthdir_x(16,i),y+lengthdir_y(16,i),z+hiz)&gt;-1)
