// hit the ground
if(zspeed&lt;=0)
{
    line1 = line_col(obj_room.list,  x,y,z+height-2,  x,y,(z)+zspeed)
    line2 = line_col(obj_room.list,  x,y,z+height-2,  x,y,z)
    
    if (line2&gt;-1){line = line2}else{line = line1}
    if (line &gt; -1){
        zspeed = 0;
        z = find_height(x,y,line)  
    }
}

// hit your head
