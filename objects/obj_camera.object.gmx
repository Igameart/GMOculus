        <whoName>self</whoName>
        <relative>0</relative>
        <isnot>0</isnot>
        <arguments>
          <argument>
            <kind>1</kind>
            <string>//Get the change of the mouse position
change_x = (display_mouse_get_x()-display_get_width()/2);
change_y = (display_mouse_get_y()-display_get_height()/2);

direction -= change_x/10;
zdirection -= change_y/10;

//Reset the mouse
display_mouse_set(display_get_width()/2,display_get_height()/2);

// set the projection 
xt=x+cos(degtorad(direction));
yt=y-sin(degtorad(direction));
zt=z+tan(degtorad(zdirection));

if (global.camera_style == CAM_REGULAR) {
    // Regular camera
    
    //Draw the sight on a surface
    surface_set_target(global.SURFACE[SURF_REGULAR]);
        draw_clear(c_black);
        d3d_set_projection(x,y,z, xt,yt,zt, 0,0,1);
        draw3DObjects();
    surface_reset_target();
    
    //Draw the surface on the screen
    d3d_set_projection_ortho(0,0,1920,1080,0);
    //draw_surface(global.SURFACE[SURF_REGULAR],0,0);
    draw_surface_ext(global.SURFACE[SURF_REGULAR],0,0,0.5,0.5,0,c_white,1);
  
} else {
    // Oculus camera
    
    //Draw the left eye on a surface
    surface_set_target(global.SURFACE[SURF_LEFT]);
        draw_clear(c_black);
        d3d_set_projection_ext(x-lengthdir_x(es,direction-90),y-lengthdir_y(es,direction-90),z,    xt,yt,zt,   0,0,1, field_of_view, cam_aspect, 1, 320000);
        draw3DObjects();

    //Draw the right eye on a surface
    surface_set_target(global.SURFACE[SURF_RIGHT]);        
        draw_clear(c_black);
        d3d_set_projection_ext(x+lengthdir_x(es,direction-90),y+lengthdir_y(es,direction-90),z,    xt,yt,zt,   0,0,1, field_of_view, cam_aspect, 1, 320000);
        draw3DObjects();

    //Go back to regular drawing and show the surfaces
    surface_reset_target();
    d3d_set_projection_ortho(0,0,1920,1080,0);
    // This is just a test drawing. We need to reshape the whole thing for the oculus.
    draw_surface_ext(global.SURFACE[SURF_LEFT],0,0,0.2,0.2,0,c_white,1);
    draw_surface_ext(global.SURFACE[SURF_RIGHT],display_get_width()/2,0,0.2,0.2,0,c_white,1);
}
</string>
          </argument>
        </arguments>
